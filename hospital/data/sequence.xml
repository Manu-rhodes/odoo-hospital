<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="active">TRUE</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
        </record>

    
        <record id="sequence_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor</field>
            <field name="active">TRUE</field>
            <field name="prefix">HD</field>
            <field name="padding">6</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>